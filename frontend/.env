# Smart PromptIQ Environment Configuration

# Backend API Configuration - Your Actual Render URL
REACT_APP_API_URL=https://smartpromptiq-backend.onrender.com/api

# Alternative without /api prefix (test both if needed):
# REACT_APP_API_URL=https://smartpromptiq-backend.onrender.com

# API Key (if your backend requires one)
REACT_APP_API_KEY=your-api-key-here

# AI Service Configuration (Optional - for direct frontend calls)
REACT_APP_OPENAI_API_KEY=your-openai-key
REACT_APP_ANTHROPIC_API_KEY=your-anthropic-key

# Stripe Configuration (for billing)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key

# App Configuration
REACT_APP_APP_NAME=Smart PromptIQ
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Analytics (Optional)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
REACT_APP_ENABLE_TEAMS=true
REACT_APP_ENABLE_AI_OPTIMIZATION=true
REACT_APP_ENABLE_BILLING=true

# Backend API URL (replace with your actual Render URL)
VITE_API_URL=https://your-app-name.onrender.com/api

# Stripe public key (when you implement billing)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here

# Other environment variables
VITE_APP_NAME=SmartPromptIQ
VITE_APP_VERSION=1.0.0